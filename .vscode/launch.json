{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug Web",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceRoot}\\src\\GrislyGrotto.Web\\bin\\Debug\\netcoreapp2.1\\GrislyGrotto.Web.dll",
            "args": [],
            "cwd": "${workspaceRoot}\\src\\GrislyGrotto.Web",
            "stopAtEntry": false,
            "internalConsoleOptions": "openOnSessionStart",
            "launchBrowser": {
                "enabled": true,
                "args": "${auto-detect-url}",
                "windows": {
                    "command": "C:\\Program Files (x86)\\Google\\Chrome\\Application\\Chrome.exe",
                    "args": "${auto-detect-url}"
                },
                "osx": {
                    "command": "open"
                },
                "linux": {
                    "command": "xdg-open"
                }
            },
            "env": {
                "ASPNETCORE_ENVIRONMENT": "Development",
                "connectionStrings:default": "Data Source=localhost;Initial Catalog=grislygrotto-2018-6-1-8-5;Persist Security Info=False;User ID=grislygrotto_user;Password=***REMOVED***;Pooling=False;MultipleActiveResultSets=False;Connection Timeout=30;"
                //"connectionStrings:default": "Server=tcp:grislygrotto.database.windows.net,1433;Data Source=grislygrotto.database.windows.net;Initial Catalog=grislygrotto;Persist Security Info=False;User ID=grislygrotto_user;Password=***REMOVED***;Pooling=False;MultipleActiveResultSets=False;Encrypt=True;TrustServerCertificate=False;Connection Timeout=30;"
            },
            "sourceFileMap": {
                "/Views": "${workspaceRoot}/Views"
            }
        }
    ]
}